{
  "name": "astroon-front-end",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build:development": "env-cmd -f .env.development yarn next build && yarn next start",
    "start": "next start",
    "lint": "next lint",
    "formate": "prettier --write \"./**/*.+(js|jsx|ts|tsx|json|css|scss|sass|md|html)\"",
